<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Derivco Orniscient</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/vis")
    <link type="text/css" href="~/Content/react-select.css" rel="stylesheet"/>
    @Scripts.Render("~/bundles/modernizr")
    <link rel="icon" href="@Url.Content("~/Content/img/favicon.ico")">
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-header">
            <a class="navbar-brand brand" href="#">
                <img alt="Brand" src="@Url.Content("~/Content/img/derivco.png")">
            </a>
            @Html.ActionLink("Orniscient", "Index", "Dashboard", new {area = ""}, new {@class = "navbar-brand heading"})
        </div>
    </div>
    @RenderBody()
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/vis")
    @Html.Partial("_GlobalUrls")
    @RenderSection("scripts", required: false)
</body>
</html>
